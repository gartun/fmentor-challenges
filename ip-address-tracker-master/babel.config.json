{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": { "version": 3 },
        "modules": false
      }
    ],
    [
       "@babel/typescript"	
    ]
  ],

  "sourceType": "unambiguous",

  "ignore": [
    "core-js"
  ],

  "env": {
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  }
}